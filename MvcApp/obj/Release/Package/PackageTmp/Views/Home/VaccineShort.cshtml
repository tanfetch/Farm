@model IEnumerable<MvcApp.Controllers.AreaCollectModel>

@{

    <fieldset>
    <legend>有逾期未注射疫苗的养户（@Model.Sum(p=>p.value)）</legend>
    @foreach (var item in Model)
    {
        <a href="javascript:void(0)" 
            onclick="newMainPanel('/Vaccine/List?injectDate_2=@DateTime.Today&areaID_0=@item.areaID','疫苗任务','icon-livepigs')"
            class="easyui-linkbutton" data-options="plain:true" style="width:80px;">
        <span>@item.areaName</span><span>:</span><span>@item.value</span>
        </a>
    }
    </fieldset>
}